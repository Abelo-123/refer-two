{
    "version": 2,
    "builds": [
      {
        "src": "app/api/telegram/bot.js",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/telegram",
        "dest": "/app/api/telegram/bot.js"
      },
      {
        "src": "/(.*)",
        "dest": "/app/$1"
      }
    ]
  }
  